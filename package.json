{
  "name": "sveltekit-changelog-renderer",
  "version": "1.0.0",
  "description": "SvelteKit example for git-changelog-manager renderer",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "keywords": ["changelog", "svelte", "sveltekit", "renderer", "example"],
  "author": "Jonathan Ellwood",
  "license": "MIT"
}
